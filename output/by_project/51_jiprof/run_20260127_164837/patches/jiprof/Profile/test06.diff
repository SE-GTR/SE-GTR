+    public void test06() throws Throwable {
+        // Clear any existing profiling data and verify the state is empty
+        Profile.clear();
+        assertTrue(Profile.getFrameList().isEmpty());
+
+        // First start should add a frame
+        int sizeBefore = Profile.getFrameList().size();
+        Profile.start("!-", "!-");
+        assertEquals(sizeBefore + 1, Profile.getFrameList().size());
+
+        // Second start should add another frame
+        sizeBefore = Profile.getFrameList().size();
+        Profile.start("thread", "!-");
+        assertEquals(sizeBefore + 1, Profile.getFrameList().size());
+
+        // End should remove (or at least not increase) the frame count for the thread
+        sizeBefore = Profile.getFrameList().size();
+        Profile.end("thread", "<T|qUi`%%RnuDRw");
+        assertTrue(Profile.getFrameList().size() <= sizeBefore);
