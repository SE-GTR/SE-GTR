+public void test08() throws Throwable {
+    InterpolatedGrayImage img = new InterpolatedGrayImage(1, 1);
+    assertEquals(1, img.X());
+    assertEquals(1, img.Y());
+    assertEquals(InterpolationMethod.NEIGHBOR, img.getInterpolationMethod());
+
+    ROI roi = new ROI();
+    assertEquals(1, roi.X());
+    assertEquals(1, roi.Y());
+    assertEquals(0, roi.ux());
+    assertEquals(0, roi.uy());
+
+    img.median(-433, roi);
+    // Verify that median does not alter dimensions or interpolation method
+    assertEquals(1, img.X());
+    assertEquals(1, img.Y());
+    assertEquals(InterpolationMethod.NEIGHBOR, img.getInterpolationMethod());
+    // Verify sideâ€‘effects of median
+    assertEquals(0, img.absSum());
+    assertEquals(0L, img.sqrSum());
+    assertEquals(0, img.addSum());
+
+    InterpolatedGrayImage result = (InterpolatedGrayImage) img.subtract(5, roi);
+    // Verify that subtract updates the original image's statistics
+    assertEquals(5, img.absSum());
+    assertEquals(-5, img.addSum());
+    assertEquals(25L, img.sqrSum());
+    // Verify that the returned image reflects the same state (same instance)
+    assertSame(img, result);
+    assertEquals(5, result.absSum());
+    assertEquals(-5, result.addSum());
+    assertEquals(25L, result.sqrSum());
+    assertEquals(InterpolationMethod.NEIGHBOR, result.getInterpolationMethod());
