+@Test(timeout = 4000)
+    public void test10() throws Throwable {
+        InterpolatedGrayImage img = new InterpolatedGrayImage(1, 1);
+        assertEquals(InterpolationMethod.NEIGHBOR, img.getInterpolationMethod());
+        assertEquals(1, img.X());
+        assertEquals(1, img.Y());
+
+        ROI roi = new ROI();
+        assertEquals(0, roi.uy());
+        assertEquals(1, roi.X());
+        assertEquals(1, roi.Y());
+
+        Integer maxRes = IIPResolutionDescriptor.MAX_RESOLUTION;
+        assertEquals(Integer.MAX_VALUE, (int) maxRes);
+
+        // clear sets all pixels to the maximum resolution value
+        Image<Integer> cleared = img.clear(maxRes);
+        assertSame(img, cleared);
+        assertEquals(Integer.MAX_VALUE, img.absSum());
+
+        // create a GrayImage copy and verify inherited state
+        GrayImage gray = new GrayImage(img);
+        assertEquals(Integer.MAX_VALUE, gray.absSum());
+        assertEquals(1, gray.X());
+        assertEquals(1, gray.Y());
+
+        // clip modifies the gray image's statistical sums
+        gray.clip(1620, 6, roi);
+        assertEquals(2624400L, gray.sqrSum());
+        assertEquals(1620, gray.addSum());
+        assertEquals(1620, gray.absSum());
+
+        // add the clipped gray image back to the interpolated image
+        InterpolatedGrayImage result = (InterpolatedGrayImage) img.add((Image<Integer>) gray);
+        assertEquals(2147482029, img.absSum());
+        assertEquals(2147482029, result.absSum());
+        assertEquals((-2147482029), img.addSum());
+        assertEquals((-2147482029), result.addSum());
+    }
