+public void test06() throws Throwable {
+    InterpolatedGrayImage img = new InterpolatedGrayImage(1, 1);
+    assertEquals(1, img.X());
+    assertEquals(1, img.Y());
+    assertEquals(InterpolationMethod.NEIGHBOR, img.getInterpolationMethod());
+
+    ROI roi = new ROI();
+    assertEquals(0, roi.ly());
+    assertEquals(1, roi.Y());
+    assertEquals(1, roi.X());
+    assertEquals(0, roi.lx());
+    assertEquals(0, roi.uy());
+    assertEquals(0, roi.ux());
+
+    // Act: median should not alter size or interpolation method, but may affect pixel sums
+    img.median(1, roi);
+    assertEquals(1, img.X());
+    assertEquals(1, img.Y());
+    assertEquals(InterpolationMethod.NEIGHBOR, img.getInterpolationMethod());
+    // Verify sideâ€‘effects of median (sums remain zero for an empty image)
+    assertEquals(0, img.absSum());
+    assertEquals(0, img.addSum());
+    assertEquals(0L, img.sqrSum());
+
+    // Act: add the image to itself
+    InterpolatedGrayImage result = (InterpolatedGrayImage) img.add((Image<Integer>) img, roi, roi);
+    // The operation should return the same instance
+    assertSame(img, result);
+    // Verify that the addition did not change dimensions or sums
+    assertEquals(1, result.X());
+    assertEquals(1, result.Y());
+    assertEquals(InterpolationMethod.NEIGHBOR, result.getInterpolationMethod());
+    assertEquals(0, result.absSum());
+    assertEquals(0, result.addSum());
+    assertEquals(0L, result.sqrSum());
+
+    // ROI should remain unchanged
+    assertEquals(0, roi.ly());
+    assertEquals(1, roi.Y());
+    assertEquals(1, roi.X());
+    assertEquals(0, roi.lx());
+    assertEquals(0, roi.uy());
+    assertEquals(0, roi.ux());
