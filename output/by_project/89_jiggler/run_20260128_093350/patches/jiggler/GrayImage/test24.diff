+public void test24() throws Throwable {
+    InterpolatedGrayImage img0 = new InterpolatedGrayImage(1, 1);
+    assertEquals(InterpolationMethod.NEIGHBOR, img0.getInterpolationMethod());
+    assertEquals(1, img0.X());
+    assertEquals(1, img0.Y());
+
+    ROI roi = new ROI();
+    assertEquals(0, roi.ly());
+    assertEquals(1, roi.Y());
+
+    // median should not alter size or statistical sums for an empty image
+    img0.median(1, roi);
+    assertEquals(0, img0.addSum());
+    assertEquals(0, img0.absSum());
+    assertEquals(0L, img0.sqrSum());
+
+    Integer dragLayer = JLayeredPane.DRAG_LAYER;
+    assertEquals(400, (int) dragLayer);
+
+    InterpolatedGrayImage img1 = new InterpolatedGrayImage(1, 1);
+    assertFalse(img1.equals(img0));
+    assertEquals(InterpolationMethod.NEIGHBOR, img1.getInterpolationMethod());
+
+    Image<Integer> cleared = img1.clear(dragLayer);
+    // clear sets all pixels to 400, affecting sums
+    assertEquals(400, img1.absSum());
+    assertEquals(160000L, img1.sqrSum());
+    assertEquals(400, img1.addSum());
+    assertSame(img1, cleared);
+    assertEquals(1, cleared.X());
+    assertEquals(1, cleared.Y());
+
+    InterpolatedGrayImage img2 = (InterpolatedGrayImage) img0.add((Image<Integer>) img1);
+    // after addition, sums should reflect combined values
+    assertEquals(400, img0.absSum());
+    assertEquals(400, img1.absSum());
+    assertEquals(400, img2.absSum());
+    assertEquals(160000L, img0.sqrSum());
+    assertEquals(160000L, img1.sqrSum());
+    assertEquals(320000L, img2.sqrSum());
+
+    // dimensions remain unchanged
+    assertEquals(1, img2.X());
+    assertEquals(1, img2.Y());
