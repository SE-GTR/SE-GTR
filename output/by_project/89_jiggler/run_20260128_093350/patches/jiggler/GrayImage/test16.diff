+public void test16() throws Throwable {
+    InterpolatedGrayImage img = new InterpolatedGrayImage(1, 1);
+    // verify construction defaults
+    assertEquals(InterpolationMethod.NEIGHBOR, img.getInterpolationMethod());
+    assertEquals(1, img.X());
+    assertEquals(1, img.Y());
+
+    Integer val = Integer.valueOf(1);
+    Image<Integer> cleared = img.clear(val);
+    // side effects of clear
+    assertEquals(1, img.absSum());
+    assertEquals(1L, img.sqrSum());
