+
+    // clip should not change sums for this range
+    img.clip(Integer.MIN_VALUE, 2);
+    assertEquals(1, img.absSum());
+    assertEquals(1L, img.sqrSum());
+    assertEquals(1, img.addSum());
+    assertSame(img, cleared);
