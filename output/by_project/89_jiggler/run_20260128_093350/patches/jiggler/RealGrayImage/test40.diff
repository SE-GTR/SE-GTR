+@Test(timeout = 4000)
+    public void test40() throws Throwable {
+        // Initialize a zero‑sized image and verify minimal constructor state
+        RealGrayImage realGrayImage0 = new RealGrayImage(0, 0);
+        assertEquals(0, realGrayImage0.X());
+        assertEquals(0, realGrayImage0.Y());
+
+        // ROI default values – keep a single representative assertion
+        ROI rOI0 = new ROI(0, 0, 0, 0);
+        assertEquals(1, rOI0.X());
+
+        // Exercise addbuffer – creates a new image with a single pixel set
+        RealGrayImage realGrayImage1 = realGrayImage0.addbuffer(1355, 1, 1355, -217, 1.0f);
+        assertNotSame(realGrayImage0, realGrayImage1);
+
+        // Verify that the new image reflects the buffer addition
+        assertEquals(1355, realGrayImage1.X());
+        assertEquals(1, realGrayImage1.Y());
+        assertEquals(1.0f, realGrayImage1.max(), 0.01f);
+        assertEquals(1.0f, realGrayImage1.min(), 0.01f);
+        assertEquals(1355.0f, realGrayImage1.absSum(), 0.01f);
+        assertEquals(1355.0f, realGrayImage1.addSum(), 0.01f);
+        assertEquals(1355.0, realGrayImage1.sqrSum(), 0.01);
+
+        // Original image should remain unchanged
+        assertEquals(0, realGrayImage0.X());
+        assertEquals(0, realGrayImage0.Y());
+
+        // Call getJavaImage twice and ensure distinct producers are returned
+        ImageProducer imageProducer0 = realGrayImage0.getJavaImage();
+        ImageProducer imageProducer1 = realGrayImage0.getJavaImage();
+        assertNotSame(imageProducer0, imageProducer1);
+        assertFalse(imageProducer0.equals(imageProducer1));
+
+        // Clip the buffered image to a range and assert the side‑effect
+        float beforeMax = realGrayImage1.max();
+        float beforeMin = realGrayImage1.min();
+        realGrayImage1.clip(1, 1414);
+        float afterMax = realGrayImage1.max();
+        float afterMin = realGrayImage1.min();
+
+        // The clipping operation should keep values within the specified bounds
+        assertTrue(afterMax <= 1414 && afterMax >= 1);
+        assertTrue(afterMin >= 1 && afterMin <= 1414);
+        // In this particular case the values were already within bounds, so they stay the same
+        assertEquals(beforeMax, afterMax, 0.01f);
+        assertEquals(beforeMin, afterMin, 0.01f);
+    }
