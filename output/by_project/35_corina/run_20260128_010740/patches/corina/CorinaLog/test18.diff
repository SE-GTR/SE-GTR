+@Test(timeout = 4000)
+    public void test18() throws Throwable {
+        // Capture the initial state of the log model
+        ListModel initialModel = CorinaLog.getLogListModel();
+        int initialSize = initialModel.getSize();
+
+        // Log a message using a CorinaLog instance
+        CorinaLog logger = new CorinaLog("testLogger");
+        String testMessage = "TestMessage";
+        logger.info(testMessage);
+
+        // Verify that the model reflects the new log entry
+        ListModel updatedModel = CorinaLog.getLogListModel();
+        int updatedSize = updatedModel.getSize();
+        assertEquals(initialSize + 1, updatedSize);
+        Object lastEntry = updatedModel.getElementAt(updatedSize - 1);
+        assertTrue(lastEntry.toString().contains(testMessage));
+    }
