+public void test47() throws Throwable {
+    AdministrativeArea administrativeArea0 = new AdministrativeArea();
+    // initial state should have no locality and no type
+    assertNull(administrativeArea0.getLocality());
+    assertNull(administrativeArea0.getType());
+
+    // setting null should leave locality as null
+    administrativeArea0.setLocality((Locality) null);
+    assertNull(administrativeArea0.getLocality());
+
+    // setting a concrete Locality should be reflected by the getter
+    Locality loc = new Locality();
+    administrativeArea0.setLocality(loc);
+    assertSame(loc, administrativeArea0.getLocality());
+
+    // resetting locality to null should clear the previously set value
+    administrativeArea0.setLocality((Locality) null);
+    assertNull(administrativeArea0.getLocality());
+
+    // ensure other collections remain unaffected by locality changes
+    assertNotNull(administrativeArea0.getAdministrativeAreaName());
+    assertTrue(administrativeArea0.getAdministrativeAreaName().isEmpty());
+}
